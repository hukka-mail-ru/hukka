<xml>

  <MainWindow>
    <color value = "black" />
    <landscape> <width value="480" />  <height value="320" />  </landscape>
  </MainWindow>

  <Scene>
    <margin>  <width value="5" />  </margin>
    <landscape>  <x value="0" />   <y value="0" />  <width value="477" />  <height value="318" />  </landscape>
  </Scene>


  <Splash>
    <landscape>  <x value="0" />   <y value="0" /> </landscape>
  </Splash>
 

  <MainMenu>
    <player>
        <font> <family value="Helvetica [Cronyx]" />  <size value="20" /> <color value="white" /> </font>
        <landscape> <x value="540" />  <y value="150" />  </landscape>
    </player>
    <rating>
        <font> <family value="Helvetica [Cronyx]" />  <size value="12" />  <color value="white" /></font>
        <landscape> <x value="540" />  <y value="190" />  </landscape>
    </rating>
  </MainMenu>
  
  <Dialogs>
     <style value="MyDialog    { background: grey; } 
                   QLineEdit   { font-size: 18px; } 
                   QLabel      { font-size: 18px; color: white; }
                   QCheckBox   { font-size: 18px; color: white; }
                   QComboBox   { font-size: 18px; }
                   QPushButton { font-size: 18px; }
                   QTableWidget { font-size: 18px; }"/> 
  </Dialogs>


  <CommonChat>
    <landscape> <x value="50" /> <y value="5" />  <width value="465" /> <height value="260" />  </landscape>
	<font>
 	    <me>             <color value="white" /> </me>
		<opponent>       <color value="#FFCC00" /> </opponent>
 	    <server>         <color value="grey" /> </server>
     </font>
	<style value="Chat      { background: black; } 
                  QTextEdit { background: black; font-size: 18px; font: italic; color: white; }
                  QTableWidget { background: black; color: white; }"/>

	<history>
	    <landscape> <x value="0" /> <y value="0" />  <width value="350" /> <height value="250" />  </landscape>
	</history>

	<userlist>
	    <landscape> <x value="355" /> <y value="0" />  <width value="110" /> <height value="250" />  </landscape>
	</userlist>

  </CommonChat>


  <TableChat>
	<landscape> <x value="5" /> <y value="1000" />  <width value="380" /> <height value="215" />  </landscape>
	<font>
 	    <me>             <color value="white" /> </me>
		<opponent>       <color value="#FFCC00" /> </opponent>
 	    <server>         <color value="grey" /> </server>
     </font>
	<style value="Chat       { background: black; } 
                  QTextEdit  { background: black; font-size: 18px; font: italic; color: white; }
                  QTableWidget { background: black; color: white; }"/> 

	<history>
		<landscape> <x value="0" /> <y value="0" />  <width value="380" /> <height value="215" />  </landscape>
	</history>

	<userlist>
		<landscape> <x value="0" /> <y value="0" />  <width value="0" /> <height value="0" />  </landscape>
	</userlist>

  </TableChat>


  <Buttons>
    <width value="120" />  <height value="106" />  <textOffset value="75" />
    <font>
      <family value="Helvetica [Cronyx]" /> <size value="12" />  <color value="white" />
    </font>

      <Exit>
        <landscape> <x value="0" />  <y value="0" />  </landscape>
      </Exit>

      <NewGame>
        <landscape> <x value="0" /> <y value="180" /> </landscape>
      </NewGame>

      <FindGame>
        <landscape> <x value="120" />  <y value="180" />  </landscape>
      </FindGame>

      <Chat>
        <landscape> <x value="240" /> <y value="180" /> </landscape>
      </Chat>

      <Options>
        <landscape> <x value="360" />  <y value="180" /> </landscape>
      </Options>


      <GameMenu>
        <landscape> <x value="20" />   <y value="0" />  </landscape>
      </GameMenu>

  </Buttons>

  <Promotion>
      <width value="50" />  <height value="50" />
  </Promotion>

  <Cell>
    <width value="40" />
  </Cell>

  <Animation>
	<timer value="500" />
  </Animation>

  <Piece>
    <width value="40" />  <height value="40" />
  </Piece>

  <CaptureBox>
	<me>       <color value="#D38842" /> <x value="0" />   <y value="273" />  <width value="158" />  <height value="20" /> </me>
	<opponent> <color value="#7C3B1B" /> <x value="0" />   <y value="295"/> <width value="158" />  <height value="20" /></opponent>
    <border> <width value="1" /> </border>
    <Piece>  <width value="13" />  <height value="11" />  </Piece>
  </CaptureBox>

  <MoveBox>

	<me>       <color> <active value="white" /> <inactive value="grey" />  </color> 
               <border> <x value="0" />   <y value="195" />  <width value="158" />  <height value="75" /> </border> 
    </me>

	<opponent> <color> <active value="#FFCC00" /> <inactive value="grey" />  </color>
               <border> <x value="0" />   <y value="118"/> <width value="158" />  <height value="75" /> </border> 
    </opponent>

    <player>     <font> <family value="Helvetica [Cronyx]" />  <size value="9" /> </font>  <x value="5" />  <y value="0"  /> </player>
    <rating>     <font> <family value="Helvetica [Cronyx]" />  <size value="8" /> </font>  <x value="5" />  <y value="15" /> </rating>
    <MoveClock>  <font> <family value="Helvetica [Cronyx]" />  <size value="15" /> </font>  <x value="5" />  <y value="25" /> </MoveClock>
    <GameClock>  <font> <family value="Helvetica [Cronyx]" />  <size value="9" /> </font>  <x value="5" />  <y value="50" /> </GameClock>

  </MoveBox>

  <Board>
    <width value="320" />
    <border>  <color value="white" />  </border>
    <x value="160" /> <y value="0" /> 
  </Board>





</xml>
